<h1>Users</h1>
<table>
  <thead>
    <th>image</th>
    <th>name</th>
    <th colspan="1"></th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <td><%= attachment_image_tag user, :profile_image, format:'jpeg', fallback: "no_image.jpg", size:"60x60" %>
    </td>
    <td><%= user.name %></td>
    <td><%= link_to "Show", user_path(user.id) %></td>
  </tbody>
    <% end %>
</table>